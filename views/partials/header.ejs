<header id="top-header">
  <h1 class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </h1>
  <% if (loggedin) { %>
    <div><a href="/account" class="welcome-link">Welcome <%= accountData.account_firstname %></a></div>
    <div><a href="/account/logout">Logout</a></div>
  <% } else { %>
    <div id="tools">
      <a title="Click to log in" href="/account/">My Account</a>
    </div>
  <% } %> <!-- Missing closing tag for else -->
</header>